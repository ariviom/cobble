{
  "name": "@brick_party/web",
  "version": "0.1.0",
  "private": "true",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write . && eslint --ext .ts,.tsx --ignore-pattern \"scripts/**\" --ignore-pattern \"coverage/**\" .",
    "format:check": "prettier --check . && eslint --ext .ts,.tsx --ignore-pattern \"scripts/**\" --ignore-pattern \"coverage/**\" .",
    "test": "vitest",
    "prepare": "simple-git-hooks",
    "ingest:rebrickable": "tsx scripts/ingest-rebrickable.ts",
    "ingest:bricklink-minifigs": "tsx scripts/ingest-bricklink-minifigs.ts",
    "build:minifig-mappings:user": "tsx scripts/build-minifig-mappings-from-user-sets.ts",
    "build:minifig-mappings:all": "tsx scripts/build-minifig-mappings-from-all-sets.ts",
    "fetch:minifig-parts": "tsx scripts/fetch-minifig-parts.ts",
    "fetch:minifig-parts:user": "MINIFIG_PARTS_USER_FIRST=true tsx scripts/fetch-minifig-parts.ts",
    "backfill:image-hashes": "tsx scripts/backfill-image-hashes.ts",
    "backfill:confidence-scores": "tsx scripts/backfill-confidence-scores.ts",
    "fix:unmapped-minifigs": "tsx scripts/fix-all-unmapped-minifigs.ts",
    "generate-types": "npx supabase gen types typescript --schema public --project-id nrrunazjjrwxaonlvvhh > supabase/types.ts"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.84.0",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-virtual": "^3.13.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "csv-parse": "^6.1.0",
    "dexie": "^4.2.1",
    "dotenv": "^17.2.3",
    "imghash": "^1.1.2",
    "lucide-react": "^0.545.0",
    "next": "^15.5.7",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-qr-code": "^2.0.18",
    "server-only": "^0.0.1",
    "sharp": "^0.34.5",
    "stripe": "^20.0.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.23.8",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@tanstack/eslint-plugin-query": "^5.91.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.19",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.1",
    "@vitest/coverage-v8": "^4.0.15",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "jsdom": "^27.0.1",
    "lint-staged": "^15.2.10",
    "prettier": "^3.6.2",
    "simple-git-hooks": "^2.11.1",
    "supabase": "^2.61.2",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5",
    "vitest": "^4.0.15"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write --ignore-unknown",
      "eslint --ext .ts,.tsx --ignore-pattern \"scripts/**\" --ignore-pattern \"coverage/**\" --fix"
    ],
    "*.{js,jsx,css,scss,md,mdx,json,yaml,yml,html}": "prettier --write --ignore-unknown"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "license": "MIT"
}
